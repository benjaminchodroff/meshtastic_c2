[DEFAULT]
# TCP or SERIAL
connection_type = SERIAL
# If TCP, specify the hostname and port
tcp_hostname = 192.168.1.100
tcp_port = 4403
serial_port = /dev/ttyUSB0

# !cmd - set channel to -1 to disable
channel_cmd = 1
# Comma separated list of allowed shell commands (exact match, no arguments)
# Leave empty or comment out to disable !cmd completely
allowed_shell_commands = uptime,whoami,df -h
status_interval = 600
log_file = meshtastic_c2.log
log_file_dir = ./log/meshtastic_c2  
log_file_level = INFO
console_log_level = INFO

# !test - set channel to -1 to disable
channel_test = 1

# MQTT Configuration (beta)
mqtt_enabled = false
mqtt_broker = mqtt.meshtastic.org
mqtt_port = 1883
mqtt_username = meshdev
mqtt_password = large4cats
mqtt_topic = msh/AE
