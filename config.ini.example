[DEFAULT]
# Comma-separated list of commands to disable (e.g., !cmd,!test)
disabled_commands = !cmd
# TCP or SERIAL
connection_type = SERIAL
# If TCP, specify the hostname and port
tcp_hostname = 192.168.1.100
tcp_port = 4403
serial_port = /dev/ttyUSB0

channel_cmd = 1
status_interval = 600
log_file = meshtastic_c2.log
log_file_dir = ./log/meshtastic_c2  
log_file_level = INFO
console_log_level = INFO
channel_test = 1

# MQTT Configuration (beta)
mqtt_enabled = false
mqtt_broker = mqtt.meshtastic.org
mqtt_port = 1883
mqtt_username = meshdev
mqtt_password = large4cats
mqtt_topic = msh/AE
